{
  "name": "truegrf",
  "version": "dev",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "cp node_modules/carbon-components-svelte/css/*.css static/ && svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. src && eslint --ignore-path .gitignore src",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. src"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "^1.0.0-next.31",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "async-mutex": "^0.3.2",
    "carbon-components-svelte": "^0.64.0",
    "carbon-icons-svelte": "^11.0.1",
    "carbon-preprocess-svelte": "^0.9.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "file-saver": "^2.0.5",
    "js-yaml": "^4.1.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "slug": "^5.3.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "truegrf": "^2.1.0",
    "tslib": "^2.4.0",
    "typescript": "~4.6.4"
  },
  "type": "module"
}
