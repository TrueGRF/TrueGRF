{
  "name": "truegrf",
  "version": "2.0.0",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "cp node_modules/carbon-components-svelte/css/*.css static/ && svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. src && eslint --ignore-path .gitignore src",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. src"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "^1.0.0-next.29",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "carbon-components-svelte": "^0.61.1",
    "carbon-icons-svelte": "^11.0.1",
    "carbon-preprocess-svelte": "^0.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "file-saver": "^2.0.5",
    "js-yaml": "^4.1.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "truegrf": "^0.2.0",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "type": "module"
}
